// usual boilerplate
doctype html
html(lang="en")
    head
        title Colour test
        script(src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.7.2/p5.js")
        script(src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.7.2/addons/p5.dom.js")
        script(src="https://cdnjs.cloudflare.com/ajax/libs/tone/13.8.6/Tone.js")
        //- link(rel="stylesheet" href="style.css")
        script(src="main.js" type="module")
    body(style="background-color: #ffffff;" onload="loader();")
        include qr.svg
    script.
        function loader(){
            document.querySelectorAll('.filled rect').forEach((x,i)=>{
                let index = Math.floor(x.getBoundingClientRect().x/460*12);
                console.log(noteColours.daze.hex[index])
                //- x.style.stroke = noteColours.daze.hex[index];
                x.style.strokeWidth = 5;
                x.style.opacity = 0.7;
                //- x.style.stroke = 'black'
                x.style.fill = noteColours.daze.hex[index];
            })
            document.querySelector('rect').style.fill='#ffffff';
        }